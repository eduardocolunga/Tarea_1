 function setup() {

  // Sets the screen to be 200 pixefunctionls wide and 200 pixels high
  createCanvas(400,400);
  background(0);
  noSmooth();

  translate(240, 0);

  // Draw lines
  stroke(255);
  line(-40, 0, -40, 200);
    stroke(255);
  line(-40, 200, 160, 400);
    stroke(255);
  line(-40, 200, -240, 400);
  
}
